{
  "name": "glittr-sdk",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish": "changeset version && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@glittr-sdk/eslint-config": "*",
    "@glittr-sdk/typescript-config": "*",
    "prettier": "^3.2.5",
    "turbo": "^1.13.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "cross-fetch": "^4.0.0"
  },
  "version": "0.0.0"
}
